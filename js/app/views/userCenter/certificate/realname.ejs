<div class="mt20 ml20">
  <h4>实名认证</h4>
</div>
<div class="user-cr">

  <form class="form-horizontal w90" role="form">
    <div class="form-group form-group-sm ">
      <label for="nickName" class="col-xs-3 control-label">真实姓名：</label>
      <div class="col-xs-6">
        <input type="text" class="form-control" id="nickName" placeholder="真实姓名"></div>
    </div>
    <div class="form-group">
      <label for="nickName" class="col-xs-3 control-label">性别：</label>
      <div class="col-sm-9">
        <label class="radio-inline">
          <input type="radio" name="inlineRadioOptions" id="man" value="option1">男士</label>
        <label class="radio-inline">
          <input type="radio" name="inlineRadioOptions" id="woman" value="option1">女士</label>
      </div>
    </div>
    <div class="form-group form-group-sm ">
      <label for="nickName" class="col-xs-3 control-label">身份证：</label>
      <div class="col-xs-6">
        <input type="text" class="form-control" id="nickName" placeholder="身份证号码"></div>
    </div>
    <div class="form-group form-group-sm ">
      <label for="nickName" class="col-xs-3 control-label">证件照：</label>
      <div class="col-xs-3">
        <input type="file" id="exampleInputFile"></div>
    </div>

    <div class="form-group">
      <div class="col-xs-offset-3 col-xs-9">
        <button type="submit" class="btn btn-primary">确认</button>
        <button type="reset" class="btn btn-default">重置</button>
      </div>
    </div>
  </form>

</div>